{
  "name": "@onflow/flow-cadut-generator",
  "amdName": "flowCadutGenerator",
  "version": "0.0.1",
  "author": "Dapper Labs",
  "license": "Apache-2.0",
  "main": "dist/generator.js",
  "module": "dist/generator.module.js",
  "source": {
    "src/index.js": {
      "cjs": "dist/generator.js",
      "esm": "dist/generator.module.js"
    },
    "src/generate.js": {
      "cjs": "dist/generate.js"
    }
  },
  "scripts": {
    "build": "fcl-bundle && printf '%s\n%s\n' \"#!/usr/bin/env node\" \"$(cat dist/generate.js)\" > dist/generate.js",
    "dev": "fcl-bundle --watch",
    "test": "jest"
  },
  "mangle": {
    "regex": "^_"
  },
  "devDependencies": {
    "@onflow/fcl-bundle": "^1.0.1-alpha.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.6",
    "@onflow/flow-cadut": "^0.2.0-alpha.7"
  },
  "bin": {
    "flow-generate": "./dist/cli.js"
  }
}
